Host *
    StrictHostKeyChecking no
    CheckHostIP no
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

Host *.lan.cogtree.com
    User emmett.butler
    ForwardAgent yes

Host *.cogtree.com
    User emmett.butler
    ForwardAgent yes

